# Basic logic functions
def AND(p, q): return p and q
def OR(p, q): return p or q
def NOT(p): return not p
def IMPLIES(p, q): return (not p) or q
# Knowledge base
P = True
Q = IMPLIES(P, True) R = IMPLIES(Q, True) # P â†’ Q
# Q â†’ R
print("Knowledge Base Results:")
print("P:"
, P,
"Q:"
, Q,
"R:"
, R)
# Example rule: P OR Q â†’ R
P, Q = False, True
R = OR(P, Q)
print("\nRule: P OR Q â†’ R")
print("P:"
, P,
"Q:"
, Q,
"P OR Q:"
, OR(P, Q),
"R:"
, R)
# Implication Example
P, Q = True, False
print("\nImplication Example:")
print("P:"
, P,
"Q:"
, Q,
"P â†’ Q:"
, IMPLIES(P, Q))
