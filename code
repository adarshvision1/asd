from collections import deque
# Graph (Adjacency List)
graph = {
'A': ['B'
,
'B': ['D'
,
'C'],
'E'],
'C': ['F'],
'D': [],
'E': ['F'],
'F': []
}
# BFS Traversal
def bfs(start):
visited, queue = set(), deque([start])
order = []
while queue:
node = queue.popleft()
if node not in visited:
visited.add(node)
order.append(node)
queue.extend(graph[node])
return order
# DFS Traversal
def dfs(start, visited=None):
if visited is None:
visited = set()
visited.add(start)
print(start, end=" ")
for neighbor in graph[start]:
if neighbor not in visited:
dfs(neighbor, visited)
# Driver code
print("BFS:"
, bfs('A')) # ['A'
,
'B'
,
print("DFS:"
, end=" ")
dfs('A') # A B D E F C
'C'
,
'D'
,
'E'
,
'F']
